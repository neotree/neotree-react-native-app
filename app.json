{
  "expo": {
    "name": "NeoTree",
    "slug": "neotree",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.4.87",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.neotree.neotreeapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "org.neotree.neotreeapp",
      "versionCode": 1
    },
    "web": {
      "build": {
        "babel": {
          "include": ["react-router-native"]
        }
      }
    }
  }
}
